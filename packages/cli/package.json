{
  "name": "dingtalk-design-cli",
  "version": "0.11.0-beta.4",
  "description": "dingtalk design cli",
  "workspaces": [
    "h5bundle/*"
  ],
  "keywords": [
    "DingTalk",
    "DingDing",
    "CLI",
    "application",
    "worktab"
  ],
  "main": "dist/bin/dd.js",
  "bin": {
    "dd": "dist/bin/dd.js",
    "ddi": "dist/bin/dd.js"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "yarn clean && npm run build:types",
    "build:types": "tsc",
    "test": "jest",
    "clean": "rimraf ./dist",
    "link:generator:global": "cd ../generator && npm link",
    "link:generator:local": "npm run link:generator:global && npm link generator-dd-application"
  },
  "author": "lou1swu",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/chalk": "^2.2.0",
    "@types/download": "^6.2.4",
    "@types/lodash": "^4.14.168",
    "@types/lru-cache": "^5.1.0",
    "@types/mkdirp": "^1.0.1",
    "@types/semver": "^7.3.4",
    "@types/tar": "^4.0.4",
    "@types/urlencode": "^1.1.2",
    "@types/yeoman-environment": "^2.10.2",
    "cpx": "^1.5.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "download": "^8.0.0",
    "events": "^3.2.0",
    "execa": "^5.0.0",
    "generator-dd-application": "latest",
    "import-global": "^0.1.0",
    "leven": "^3.1.0",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "mkdirp": "^1.0.4",
    "open": "^8.0.6",
    "ora": "^5.3.0",
    "semver": "^7.3.4",
    "strip-ansi": "^6.0.0",
    "tar": "^6.1.0",
    "urlencode": "^1.1.0",
    "yeoman-environment": "^2.10.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=12.15.x"
  }
